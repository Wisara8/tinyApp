<h1>Welcome to tinyApp</h1>
<% if (!user ) { %>

   <form action="/login" method="GET" style="margin:150px;">
    <input type="submit" value="Login">
  </form>
   <form action="/register" method="GET" style="margin:150px;">
    <input type="submit" value="Register">
  </form>

 <% } else { %>
   Welcome <%= user.email %>
  <form action="/logout" method="POST" style="margin:150px;">
    <input type="submit" value="Logout">
  </form>
<% } %>


